

generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model User {
  id         Int      @id @default(autoincrement())
  name       String
  age        Int?
  email      String   @unique
  password   String
  mobile     String?
  altMobile  String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}


model Post {
  id         Int      @id @default(autoincrement())
  title      String
  
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
